{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
    "parameters": {
          "VMloginName":{
            "type": "string"
          },
          "KeyVaultID":{
            "type": "string"
          },
          "SecretName":{
            "type": "string",
            "defaultValue": "VM"
          }

    },
  "variables": {
    "_artifactsLocation":"https://raw.githubusercontent.com/Nerwoolf/azure_arm/test/"
   },
  "resources": [
  {
      "type": "Microsoft.Resources/deployments",
      "apiVersion": "2015-01-01",
      "name": "NestedDeployment1",
      "properties": {
          "mode": "Incremental",
          "templateLink": {
              "uri": "[concat(variables('_artifactsLocation'),'vm.json')]",
              "contentVersion": "1.0.0.0"
          },
          "parameters": {
            "loginName":{
              "value": "[parameters('VMloginName')]"
            },
            "password":{
              "reference": {
                "keyVault": {
                    "id": "[parameters('KeyVaultID')]"
                },
                "secretName": "[parameters('SecretName')]"
            }
            }
          }
      }
  }
  ],
  "outputs": {}
}
