{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
  },
  "variables": {
    "virtnetPrefix": "10.0.0.0/16",
    "virtnetSubnet1Name": "FrontEndSubnet",
    "virtnetSubnet1Prefix": "10.0.0.0/24",
    "virtnetSubnet2Name": "BackEndSubnet",
    "virtnetSubnet2Prefix": "10.0.1.0/24"
  },
  "resources": [
    {
      "name": "virtnet",
      "type": "Microsoft.Network/virtualNetworks",
      "location": "[resourceGroup().location]",
      "apiVersion": "2016-03-30",
      "dependsOn": [ ],
      "tags": {
        "displayName": "virtnet"
      },
      "properties": {
        "addressSpace": {
          "addressPrefixes": [
            "[variables('virtnetPrefix')]"
          ]
        },
        "subnets": [
          {
            "name": "[variables('virtnetSubnet1Name')]",
            "properties": {
              "addressPrefix": "[variables('virtnetSubnet1Prefix')]"
            }
          },
          {
            "name": "[variables('virtnetSubnet2Name')]",
            "properties": {
              "addressPrefix": "[variables('virtnetSubnet2Prefix')]"
            }
          }
        ]
      }
    }
  ],
  "outputs": {
  }
}
