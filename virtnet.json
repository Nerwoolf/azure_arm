{
  "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "VNetName":{
        "type":"string",
        "defaultValue": "[concat(resourceGroup().name,'-vnet')]",
        "metadata":{
          "description":"Virtual network name"
        }
      },
    "VNetAddressPrefix":{
        "type":"string",
        "defaultValue": "192.168.0.0/16"
      },
    "FEsubnetName":{
        "defaultValue": "FESubnet",
        "type":"string"
      },
    "BESubnetName":{
      "type": "string",
      "defaultValue": "BESubnet"
    },
    "FEaddressPrefix":{
        "type":"string",
        "defaultValue": "192.168.0.0/24"
      },
    "BEaddressPrefix":{
        "type":"string",
        "defaultValue": "192.168.1.0/24"
    }
  },
  "variables": {

  },
  "resources": [
    {
      "name": "[parameters('VNetName')]",
      "type": "Microsoft.Network/virtualNetworks",
      "location": "[resourceGroup().location]",
      "apiVersion": "2016-03-30",
      "dependsOn": [ ],
      "tags": {
        "displayName": "virtnet"
      },
      "properties": {
        "addressSpace": {
          "addressPrefixes": [
            "[parameters('VNetAddressPrefix')]"
          ]
        },
        "subnets": [
          {
            "name": "[parameters('BESubnetName')]",
            "properties": {
              "addressPrefix": "[parameters('BEaddressPrefix')]"
            }
          },
          {
            "name": "[parameters('FEaddressPrefix')]",
            "properties": {
              "addressPrefix": "[parameters('FEaddressPrefix')]"
            }
          }
        ]
      }
    }
  ],
  "outputs": {
  }
}
